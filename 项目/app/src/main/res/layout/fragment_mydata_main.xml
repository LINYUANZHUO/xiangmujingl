<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical"
	android:background="@android:color/white"
	android:layout_width="match_parent"
	android:layout_height="match_parent">

	<LinearLayout
		android:gravity="center_vertical"
		android:orientation="horizontal"
		android:id="@+id/fragmentmydatamainLinearLayout1"
		android:background="?android:attr/selectableItemBackground"
		android:clickable="true"
		android:layout_width="match_parent"
		android:layout_height="50dp">

		<ImageView
			android:layout_width="25dp"
			android:layout_height="25dp"
			android:layout_marginLeft="15dp"
			android:src="@drawable/ic_create_black_24dp"/>

		<TextView
			android:textSize="18sp"
			android:textColor="#FF000000"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="10dp"
			android:text="我的小岛/专题/文集/"
			android:layout_weight="1.0"/>

		<TextView
			android:id="@+id/fragmentmydatamainTextView1"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginRight="5dp"
			android:text="0"/>

		<ImageView
			android:layout_width="25dp"
			android:layout_height="25dp"
			android:layout_marginRight="15dp"
			android:src="@drawable/ic_c"/>

	</LinearLayout>

	<LinearLayout
		android:gravity="center_vertical"
		android:orientation="horizontal"
		android:id="@+id/fragmentmydatamainLinearLayout2"
		android:background="?android:attr/selectableItemBackground"
		android:clickable="true"
		android:layout_width="match_parent"
		android:layout_height="50dp">

		<ImageView
			android:layout_width="25dp"
			android:layout_height="25dp"
			android:layout_marginLeft="15dp"
			android:src="@drawable/ic_sms_black_24dp"/>

		<TextView
			android:textSize="18sp"
			android:textColor="#FF000000"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="10dp"
			android:text="我的帖子/回复/消息"
			android:layout_weight="1.0"/>

		<TextView
			android:id="@+id/fragmentmydatamainTextView2"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginRight="5dp"
			android:text="0"/>

		<ImageView
			android:layout_width="25dp"
			android:layout_height="25dp"
			android:layout_marginRight="15dp"
			android:src="@drawable/ic_c"/>

	</LinearLayout>

	<LinearLayout
		android:gravity="center_vertical"
		android:orientation="horizontal"
		android:id="@+id/fragmentmydatamainLinearLayout3"
		android:background="?android:attr/selectableItemBackground"
		android:clickable="true"
		android:layout_width="match_parent"
		android:layout_height="50dp">

		<ImageView
			android:layout_width="25dp"
			android:layout_height="25dp"
			android:layout_marginLeft="15dp"
			android:src="@drawable/ic_star_black_24dp"/>

		<TextView
			android:textSize="18sp"
			android:textColor="#FF000000"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="10dp"
			android:text="我的收藏"
			android:layout_weight="1.0"/>

		<TextView
			android:id="@+id/fragmentmydatamainTextView3"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginRight="5dp"
			android:text="0"/>

		<ImageView
			android:layout_width="25dp"
			android:layout_height="25dp"
			android:layout_marginRight="15dp"
			android:src="@drawable/ic_c"/>

	</LinearLayout>

	<View
		android:background="?android:attr/dividerVertical"
		android:layout_width="match_parent"
		android:layout_height="0.5dp"
		android:layout_marginLeft="50dp"/>

	<LinearLayout
		android:gravity="center_vertical"
		android:orientation="horizontal"
		android:id="@+id/fragmentmydatamainLinearLayout4"
		android:background="?android:attr/selectableItemBackground"
		android:clickable="true"
		android:layout_width="match_parent"
		android:layout_height="50dp">

		<ImageView
			android:layout_width="25dp"
			android:layout_height="25dp"
			android:layout_marginLeft="15dp"
			android:src="@drawable/ic_color_lens_black_24dp"/>

		<TextView
			android:textSize="18sp"
			android:textColor="#FF000000"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="10dp"
			android:text="切换主题"
			android:layout_weight="1.0"/>

		<ImageView
			android:layout_width="25dp"
			android:layout_height="25dp"
			android:layout_marginRight="15dp"
			android:src="@drawable/ic_c"/>

	</LinearLayout>

	<LinearLayout
		android:gravity="center_vertical"
		android:orientation="horizontal"
		android:id="@+id/fragmentmydatamainLinearLayout5"
		android:background="?android:attr/selectableItemBackground"
		android:clickable="true"
		android:layout_width="match_parent"
		android:layout_height="50dp">

		<ImageView
			android:layout_width="25dp"
			android:layout_height="25dp"
			android:layout_marginLeft="15dp"
			android:src="@drawable/ic_settings_black"/>

		<TextView
			android:textSize="18sp"
			android:textColor="#FF000000"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="10dp"
			android:text="设置"
			android:layout_weight="1.0"/>

		<ImageView
			android:layout_width="25dp"
			android:layout_height="25dp"
			android:layout_marginRight="15dp"
			android:src="@drawable/ic_c"/>

	</LinearLayout>

</LinearLayout>

